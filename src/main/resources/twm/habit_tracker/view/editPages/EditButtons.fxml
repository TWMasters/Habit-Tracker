<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<ButtonBar fx:id="buttonBarReference"
           xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="twm.habit_tracker.controller.editPages.ButtonController"
           prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
    <buttons>
        <Button fx:id="backButton" mnemonicParsing="false" text="Back" onAction="#backButtonPush" />
        <Button fx:id="saveButton" mnemonicParsing="false"  text="Save" onAction="#saveButtonPush"/>
        <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" onAction="#deleteButtonPush" />
    </buttons>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</ButtonBar>

