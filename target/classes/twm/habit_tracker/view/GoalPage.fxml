        <ButtonBar BorderPane.alignment="TOP_CENTER">
            <buttons>
                <Label text="GOALS">
                    <font>
                        <Font size="48.0" />
                    </font>
                </Label>
                <Region prefHeight="200.0" prefWidth="200.0" />
