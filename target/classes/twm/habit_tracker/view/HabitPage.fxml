<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="twm.habit_tracker.view.mainPages.HabitPageController">
   <top>
      <ButtonBar BorderPane.alignment="TOP_CENTER">
        <buttons>
            <Label text="HABITS">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <Region prefHeight="200.0" prefWidth="200.0" />
          <Button mnemonicParsing="false" onAction="#addButtonPush" text="+" />
        </buttons>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </ButtonBar>
   </top>
   <center>
      <StackPane BorderPane.alignment="TOP_LEFT">
         <children>
            <TableView fx:id="Habits_Table" onMouseClicked="#editTablePush">
              <columns>
                <TableColumn text="Habit" />
                <TableColumn text="Question" />
              </columns>
            </TableView>
         </children>
      </StackPane>
   </center>
</BorderPane>
